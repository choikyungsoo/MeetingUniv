<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.meetinguniv.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_mainScreenFragment"
            app:destination="@id/mainScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_shopScreenFragment"
            app:destination="@id/shopScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_settingsScreenFragment2"
            app:destination="@id/settingsScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_chattingContentFragment"
            app:destination="@id/chattingScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_friendsListScreenFragment"
            app:destination="@id/friendsListScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_chatRoomScreenFragment"
            app:destination="@id/chatRoomScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_friendsProfileScreenFragment"
            app:destination="@id/friendsProfileScreenFragment" />
        <action
            android:id="@+id/action_mainFragment_to_personalProfileScreenFragment"
            app:destination="@id/personalProfileScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainScreenFragment"
        android:name="com.example.meetinguniv.main.MainScreenFragment"
        android:label="fragment_main_screen"
        tools:layout="@layout/fragment_main_screen" >
        <action
            android:id="@+id/action_mainScreenFragment_to_matchingContentsFragment"
            app:destination="@id/matchingContentsFragment" />
        <action
            android:id="@+id/action_mainScreenFragment_to_currentContentsFragment"
            app:destination="@id/currentContentsFragment" />
    </fragment>
    <fragment
        android:id="@+id/shopScreenFragment"
        android:name="com.example.meetinguniv.main.ShopScreenFragment"
        android:label="fragment_shop_screen"
        tools:layout="@layout/fragment_shop_screen" />
    <fragment
        android:id="@+id/matchingContentsFragment"
        android:name="com.example.meetinguniv.main.MatchingContentFragment"
        android:label="fragment_matching_contents"
        tools:layout="@layout/fragment_matching_content" >
        <action
            android:id="@+id/action_matchingContentsFragment_to_chooseteamElementFragment"
            app:destination="@id/chooseteamElementFragment" />
    </fragment>
    <fragment
        android:id="@+id/currentContentsFragment"
        android:name="com.example.meetinguniv.main.CurrentContentsFragment"
        android:label="fragment_current_contents"
        tools:layout="@layout/fragment_current_content" />
    <fragment
        android:id="@+id/chooseteamElementFragment"
        android:name="com.example.meetinguniv.main.chooseteamElementFragment"
        android:label="fragment_chooseteam_element"
        tools:layout="@layout/fragment_chooseteam_element" />

    <fragment
        android:id="@+id/settingsScreenFragment"
        android:name="com.example.meetinguniv.main.SettingsScreenFragment"
        android:label="fragment_settings_screen"
        tools:layout="@layout/fragment_settings_screen" />
    <fragment
        android:id="@+id/chatRoomScreenFragment"
        android:name="com.example.meetinguniv.main.ChatRoomScreenFragment"
        android:label="fragment_chat_room_screen"
        tools:layout="@layout/fragment_chat_room_screen" />
    <fragment
        android:id="@+id/personalChattingContentsFragment"
        android:name="com.example.meetinguniv.main.PersonalChattingContentFragment"
        android:label="fragment_personal_chating_contents"
        tools:layout="@layout/fragment_personal_chatting_content" >
        <action
            android:id="@+id/action_personalChattingContentsFragment_to_chatRoomScreenFragment"
            app:destination="@id/chatRoomScreenFragment" />
        <action
            android:id="@+id/action_personalChattingContentsFragment_to_floatingFriendsListContentFragment"
            app:destination="@id/floatingFriendsListContentFragment" />
    </fragment>
    <fragment
        android:id="@+id/matchChattingContentsFragment2"
        android:name="com.example.meetinguniv.main.MatchChattingContentFragment"
        android:label="fragment_match_chatting_contents"
        tools:layout="@layout/fragment_match_chatting_content" >
        <action
            android:id="@+id/action_matchChattingContentsFragment2_to_chatRoomScreenFragment"
            app:destination="@id/chatRoomScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/chattingScreenFragment"
        android:name="com.example.meetinguniv.main.ChattingScreenFragment"
        android:label="ChattingScreenFragment" >
        <action
            android:id="@+id/action_chattingScreenFragment_to_matchChattingContentsFragment2"
            app:destination="@id/matchChattingContentsFragment2" />
        <action
            android:id="@+id/action_chattingScreenFragment_to_personalChattingContentsFragment"
            app:destination="@id/personalChattingContentsFragment" />
    </fragment>
    <fragment
        android:id="@+id/friendsListScreenFragment"
        android:name="com.example.meetinguniv.main.FriendsListScreenFragment"
        android:label="fragment_friends_list_screen"
        tools:layout="@layout/fragment_friends_list_screen" >
        <action
            android:id="@+id/action_friendsListScreenFragment_to_friendsProfileScreenFragment"
            app:destination="@id/friendsProfileScreenFragment" />
        <action
            android:id="@+id/action_friendsListScreenFragment_to_personalProfileScreenFragment"
            app:destination="@id/personalProfileScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/friendsProfileScreenFragment"
        android:name="com.example.meetinguniv.FriendsProfileScreenFragment"
        android:label="fragment_friends_profile_screen"
        tools:layout="@layout/fragment_friends_profile_screen" >
        <action
            android:id="@+id/action_friendsProfileScreenFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/personalProfileScreenFragment"
        android:name="com.example.meetinguniv.PersonalProfileScreenFragment"
        android:label="fragment_personal_profile_screen"
        tools:layout="@layout/fragment_personal_profile_screen" />
    <fragment
        android:id="@+id/floatingFriendsListContentFragment"
        android:name="com.example.meetinguniv.FloatingFriendsListContentFragment"
        android:label="fragment_floating_friends_list_content"
        tools:layout="@layout/fragment_floating_friends_list_content" />

</navigation>