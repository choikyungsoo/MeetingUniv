<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_login"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/login"
        android:name="com.example.meetinguniv.login.LoginFragment"
        android:label="login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_join_profile"
            app:destination="@id/join_profile" />
    </fragment>

    <fragment
        android:id="@+id/join_profile"
        android:name="com.example.meetinguniv.login.JoinProfileFragment"
        android:label="login"
        tools:layout="@layout/fragment_join_profile" />
</navigation>