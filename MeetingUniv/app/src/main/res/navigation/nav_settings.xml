<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_settings"
    app:startDestination="@id/settingsContentFragment">
    <fragment
        android:id="@+id/settingsContentFragment"
        android:name="com.example.meetinguniv.main.SettingsContentFragment"
        android:label="SettingsScreenFragment" >
        <action
            android:id="@+id/action_settingsContentFragment_to_alarmSettingElementFragment"
            app:destination="@id/alarmSettingElementFragment" />
        <action
            android:id="@+id/action_settingsContentFragment_to_serviceInfoContentFragment"
            app:destination="@id/serviceInfoContentFragment" />
    </fragment>
    <fragment
        android:id="@+id/alarmSettingElementFragment"
        android:name="com.example.meetinguniv.main.AlertSettingElementFragment"
        android:label="AlertSettingElementFragment" />
    <fragment
        android:id="@+id/serviceInfoContentFragment"
        android:name="com.example.meetinguniv.ServiceInfoContentFragment"
        android:label="ServiceInfoContentFragment" >
        <action
            android:id="@+id/action_serviceInfoContentFragment_to_serviceAgreementInfoScreenFragment"
            app:destination="@id/serviceAgreementInfoScreenFragment" />
        <action
            android:id="@+id/action_serviceInfoContentFragment_to_personalInfoAgreementScreenFragment"
            app:destination="@id/personalInfoAgreementScreenFragment" />
        <action
            android:id="@+id/action_serviceInfoContentFragment_to_locationInfoAgreementScreenFragment"
            app:destination="@id/locationInfoAgreementScreenFragment" />
        <action
            android:id="@+id/action_serviceInfoContentFragment_to_openSourceLicenseInfoScreenFragment"
            app:destination="@id/openSourceLicenseInfoScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/serviceAgreementInfoScreenFragment"
        android:name="com.example.meetinguniv.ServiceAgreementInfoScreenFragment"
        android:label="ServiceAgreementInfoScreenFragment" />
    <fragment
        android:id="@+id/personalInfoAgreementScreenFragment"
        android:name="com.example.meetinguniv.PersonalInfoAgreementScreenFragment"
        android:label="fragment_personal_info_agreement_screen"
        tools:layout="@layout/fragment_personal_info_agreement_screen" />
    <fragment
        android:id="@+id/locationInfoAgreementScreenFragment"
        android:name="com.example.meetinguniv.LocationInfoAgreementScreenFragment"
        android:label="fragment_location_info_agreement_screen"
        tools:layout="@layout/fragment_location_info_agreement_screen" />
    <fragment
        android:id="@+id/openSourceLicenseInfoScreenFragment"
        android:name="com.example.meetinguniv.OpenSourceLicenseInfoScreenFragment"
        android:label="fragment_open_source_license_info_screen"
        tools:layout="@layout/fragment_open_source_license_info_screen" />
</navigation>